{
  "version": 2,
  "name": "paneladecasa-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@next_public_api_url"
    }
  },
  "functions": {
    "apps/web/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  }
}